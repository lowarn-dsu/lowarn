with-compiler: ghc-9.2.7
write-ghc-environment-files: always
packages:
    */*.cabal
  , examples/*/demo/*.cabal
  , examples/*/versions/*/*.cabal
  , examples/*/updates/*/*.cabal

flags: +cabal

package *
  ghc-options: -rdynamic -fwhole-archive-hs-libs -fkeep-cafs

package lowarn-version-reproduction-v1v0v0
  ghc-options: -fno-keep-cafs

package lowarn-version-reproduction-v2v0v0
  ghc-options: -fno-keep-cafs
